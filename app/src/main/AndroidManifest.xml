<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paperlessquiz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".B_LoginMain"></activity>
        <activity android:name=".D_ParticipantHome" />
        <activity android:name=".D_PC_ConfirmSubmitAnswers" />
        <activity android:name=".D_PB_ShowRoundQuestions" />
        <activity android:name=".D_PA_ShowRounds" />
        <activity android:name=".old_B_SelectLoginName" />
        <activity android:name=".A_SelectRole" />
        <activity android:name=".old_C_LogInToQuiz" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>